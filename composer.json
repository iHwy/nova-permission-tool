{
    "name": "lizhineng/nova-permission-tool",
    "description": "A Laravel Nova tool based on Spatie permission package.",
    "keywords": [
        "laravel",
        "nova",
        "permission",
        "tool"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "spatie/laravel-permission": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Lizhineng\\PermissionTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lizhineng\\PermissionTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
